{
  "name": "events",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://localhost:3000/app",
  "dependencies": {
    "history": "^4.7.2",
    "node-sass-chokidar": "^1.2.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
